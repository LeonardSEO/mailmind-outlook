        "mini-css-extract-plugin": "^2.7.6",
        "office-addin-dev-certs": "^1.13.5",
        "postcss": "^8.4.31",
        "postcss-loader": "^7.3.3",
        "rimraf": "^5.0.5",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.5.1",
        "typescript": "^5.2.2",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.2.0",
        "webpack-merge": "^5.10.0"
    }
