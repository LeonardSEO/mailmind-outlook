{
    "name": "outlook-addin-using-react-demo",
    "description": "",
    "author": "",
    "version": "1.0.0",
    "scripts": {
        "clean": "rimraf dist",
        "lint": "eslint src --ext .ts,.tsx",
        "dev": "webpack serve --mode development --config config/webpack.dev.js --progress",
        "build": "webpack --mode production --config config/webpack.prod.js --progress",
        "start": "npm run dev",
        "validate": "npx office-addin-manifest validate manifest.xml",
        "sideload": "start https://outlook.office.com/mail/options/mail/addins",
        "certs": "npx office-addin-dev-certs install --days 365",
        "certs:verify": "npx office-addin-dev-certs verify"
    },
    "dependencies": {
        "@fluentui/react": "^8.112.9",
        "@microsoft/office-js-helpers": "^1.0.2",
        "core-js": "^3.33.3",
        "openai": "^4.20.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/office-js": "^1.0.360",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@types/webpack": "^5.28.5",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "autoprefixer": "^10.4.16",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.8.1",
        "eslint": "^8.53.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.3",
        "less": "^4.2.0",
